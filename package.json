{
  "name": "swipe-view",
  "version": "0.1.0",
  "scripts": {
    "start": "node scripts/npm-start.js",
    "upload": "kintone-plugin-uploader dist/kintone-plugin-swipe-view.zip --watch --waiting-dialog-ms 3000",
    "develop": "npm run build -- --watch",
    "build": "kintone-plugin-packer --ppk private.ppk --out dist/kintone-plugin-swipe-view.zip src",
    "lint": "eslint src/js",
    "build-css1": "node-sass src/scss/sv-detail.scss src/css/sv-detail.css --watch",
    "build-css2": "node-sass src/scss/sv-config.scss src/css/sv-config.css --watch"
  },
  "devDependencies": {
    "@kintone/plugin-packer": "^2.0.0",
    "@kintone/plugin-uploader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-kintone": "^1.3.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5"
  }
}
